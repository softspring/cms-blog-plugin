{
    "name": "softspring/cms-blog-bundle",
    "description": "A Blog extension for softspring cms",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "softspring/cms-bundle": "^5.1@dev",
        "scienta/doctrine-json-functions": "^5.2",
        "softspring/time-ago-bundle": "^5.1@dev"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.9"
    },
    "suggest": {
        "softspring/user-bundle": "If you want to use users as blog authors"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\CmsBlogBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-5.2": "5.2.x-dev"
        }
    }
}
