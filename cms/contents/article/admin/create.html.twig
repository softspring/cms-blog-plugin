{% extends '@SfsCms/admin/content/create.html.twig' %}

{% block form_fields %}
    {{ form_row(form.name) }}
    {{ form_row(form.sites) }}
    {% if form.extraData is defined %}{{ form_widget(form.extraData) }}{% endif %}
    {{ form_row(form.publishedAt) }}
    {{ form_label(form.routes) }}
    {{ form_widget(form.routes.0) }}
    {{ form_rest(form) }}
{% endblock form_fields %}